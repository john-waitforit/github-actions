    "next-images": "^1.4.0",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^5.4.5",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.1.1",
    "superagent": "^5.3.1",
    "typesafe-actions": "^5.1.0",
    "wait-on": "^5.1.0"
